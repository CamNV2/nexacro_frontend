                      <Cell col="8" displaytype="progressbarcontrol" text="0" padding="12px 0px" font="10px/normal &quot;Malgun Gothic&quot;" color="#00000"/>
                      <Cell col="9" color="#00000"/>
                      <Cell col="10" text="bind:TOTAL_ISSUE_COMPLETE" color="#00000"/>
