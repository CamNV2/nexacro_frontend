                      <Cell col="4" text="bind:TOTAL_TASK_COMPELTE" color="#00000"/>
                      <Cell col="5" displaytype="progressbarcontrol" text="EXPR: parseInt((TOTAL_TASK_COMPELTE/TOTAL_TASK)*100) + &quot;%&quot;" maskeditformat="#" progressbarsmooth="true" progressbardirection="forward" padding="12px 0px" color="#000000"/>
                      <Cell col="6" text="bind:TOTAL_TASK" color="#00000"/>
